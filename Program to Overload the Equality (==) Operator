#include <iostream>
using namespace std;

class Complex {
    int real, imag;
public:
    Complex(int r, int i): real(r), imag(i) {}
    bool operator==(const Complex &c) {
        return (real == c.real && imag == c.imag);
    }
};

int main() {
    Complex c1(3, 4), c2(3, 4);
    if (c1 == c2)
        cout << "Complex numbers are equal";
    else
        cout << "Complex numbers are not equal";
    return 0;
}
