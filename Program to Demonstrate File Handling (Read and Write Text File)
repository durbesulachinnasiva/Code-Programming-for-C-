#include <iostream>
#include <fstream>
using namespace std;

int main() {
    ofstream fout("sample.txt");
    fout << "Hello, File Handling in C++!\n";
    fout.close();

    ifstream fin("sample.txt");
    string line;
    while (getline(fin, line))
        cout << line << endl;
    fin.close();
    return 0;
}
