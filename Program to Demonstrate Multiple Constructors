#include <iostream>
using namespace std;

class Rectangle {
    int l, b;
public:
    Rectangle() { l = b = 0; }
    Rectangle(int a, int c) { l = a; b = c; }
    void area() { cout << "Area: " << l*b << endl; }
};

int main() {
    Rectangle r1, r2(5, 10);
    r1.area();
    r2.area();
    return 0;
}
