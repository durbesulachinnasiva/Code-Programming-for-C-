#include <iostream>
using namespace std;

int main() {
    double a, b;
    char op;
    cout << "Enter expression (a + b): ";
    cin >> a >> op >> b;

    switch(op) {
        case '+': cout << a+b; break;
        case '-': cout << a-b; break;
        case '*': cout << a*b; break;
        case '/': cout << a/b; break;
        default: cout << "Invalid Operator";
    }
    return 0;
}
